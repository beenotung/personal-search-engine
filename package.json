    "nyc": "^15.1.0",
    "open-cli": "^7.0.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.2",
    "ts-mocha": "^8.0.0",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.2"
  }
