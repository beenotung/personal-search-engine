    "debug": "^4.3.2",
    "express": "^4.17.1",
    "listening-on": "^2.0.2",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.4.0",
    "@types/chai": "^4.2.21",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/integer": "^4.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "*",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "chai": "^4.3.4",
